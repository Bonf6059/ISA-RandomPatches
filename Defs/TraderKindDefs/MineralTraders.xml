<?xml version="1.0" encoding="utf-8"?>
<Defs>

    <!-- Caravan -->
    
<TraderKindDef>
    <defName>ISA_Caravan_Minerals</defName>
    <label>mineral trader</label>
	  <commonality>3</commonality>
    <stockGenerators>
        
      <li Class="StockGenerator_SingleDef">
        <thingDef>Silver</thingDef>
        <countRange>3500~5000</countRange>
      </li>
      
      <li Class="StockGenerator_SingleDef">
        <thingDef>MedicineHerbal</thingDef>
        <countRange>15~20</countRange>
      </li>
            
      <li Class="StockGenerator_SingleDef">
        <thingDef>MedicineIndustrial</thingDef>
        <countRange>15~20</countRange>
      </li>

      <li Class="StockGenerator_SingleDef">
        <thingDef>Chemfuel</thingDef>
        <countRange>100~250</countRange>
      </li>
      
      <li Class="StockGenerator_SingleDef">
        <thingDef>MealSurvivalPack</thingDef>
        <countRange>10~25</countRange>
      </li>
      
      <li Class="StockGenerator_SingleDef">
        <thingDef>Pemmican</thingDef>
        <countRange>30~75</countRange>
      </li>
      
      <li Class="StockGenerator_Category">
        <categoryDef>StoneBlocks</categoryDef>
      </li>

      <li Class="StockGenerator_SingleDef">
        <thingDef>Steel</thingDef>
        <countRange>50~125</countRange>
      </li>
      
      <li Class="StockGenerator_SingleDef">
        <thingDef>Plasteel</thingDef>
        <countRange>50~125</countRange>
      </li>
      
      <li Class="StockGenerator_SingleDef">
        <thingDef>Uranium</thingDef>
        <countRange>50~125</countRange>
      </li>
      
      <li Class="StockGenerator_SingleDef">
        <thingDef>Gold</thingDef>
        <countRange>70~240</countRange>
      </li>

      <li Class="StockGenerator_SingleDef">
        <thingDef>Jade</thingDef>
        <countRange>30~140</countRange>
      </li>

      <li Class="StockGenerator_SingleDef">
        <thingDef>InsectJelly</thingDef>
        <countRange>-10~75</countRange>
      </li>
      
      <li Class="StockGenerator_SingleDef">
        <thingDef>Meat_Megaspider</thingDef>
        <countRange>-30~141</countRange>
      </li>
      
      <li MayRequire="det.epochspyrinth" Class="StockGenerator_SingleDef">
        <thingDef>DV_Pyrinth</thingDef>
        <countRange>-10~84</countRange>
      </li>
      
      <li MayRequireAnyOf="det.epochspottery,det.epochsgolems" Class="StockGenerator_SingleDef">
        <thingDef>DV_Clay</thingDef>
        <countRange>-10~84</countRange>
      </li>

      <!-- Buying -->
      
      <li Class="StockGenerator_BuyExpensiveSimple" />
      <li Class="StockGenerator_BuyTradeTag"><tag>Art</tag></li>
      <li Class="StockGenerator_BuyTradeTag"><tag>MusicalInstrument</tag></li>
      <li Class="StockGenerator_BuyTradeTag"><tag>ExoticMisc</tag></li>
      <li Class="StockGenerator_BuyTradeTag"><tag>ResourcesRaw</tag></li>
      <li Class="StockGenerator_BuyTradeTag"><tag>Drugs</tag></li>
      
    </stockGenerators>
  </TraderKindDef>

